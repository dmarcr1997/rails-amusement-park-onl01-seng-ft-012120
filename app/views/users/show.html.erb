<% if flash[:message] %>
  <div class="center alert">
    <p><%= flash[:message] %></p>
  </div>
<% end %>


<%if @user.admin == false%>
<p><strong>Name: </strong><%=@user.name%></p>
<p><strong>Tickets: </strong><%=@user.tickets%></p>
<p><strong>Mood: </strong><%=@user.mood%></p>
<p><strong>Height: </strong><%=@user.height%></p>
<%else%>
<p><strong>ADMIN: </strong><%=@user.name%></p>
<%end%>
<p><%= link_to "See attractions", attractions_path%></p>

